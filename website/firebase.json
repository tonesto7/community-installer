{
    "database": {
        "rules": "database.rules.json"
    },
    "hosting": {
        "public": "public",
        "headers": [{
            "source": "**",
            "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            }]
        }],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }]
    }
}